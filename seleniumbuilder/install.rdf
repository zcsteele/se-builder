<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

    <Description about="urn:mozilla:install-manifest">
        <em:id>seleniumbuilder@saucelabs.com</em:id>
        <em:name>Selenium Builder</em:name>
        <em:version>2.3.2</em:version>
        <em:type>2</em:type>
        <em:creator>Selenium Builder</em:creator>
        <em:developer>David Stark</em:developer>
        <em:description>Create automated cross-browser functional tests easily and quickly.</em:description>
      	<em:iconURL>chrome://seleniumbuilder/content/html/img/builder-logo.png</em:iconURL>
        <em:homepageURL>http://www.sebuilder.com/</em:homepageURL>
        <em:updateURL>https://seleniumbuilder.github.io/se-builder/update.rdf</em:updateURL>
        <em:unpack>true</em:unpack>

        <em:targetApplication>
            <Description>
                <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
                <em:minVersion>3.6</em:minVersion>
                <em:maxVersion>37.*</em:maxVersion>
            </Description>
        </em:targetApplication>
        
        <!-- Platforms where we're not compiling the native events -->
        <em:targetPlatform>Darwin</em:targetPlatform>
        <em:targetPlatform>SunOS</em:targetPlatform>
        <em:targetPlatform>FreeBSD</em:targetPlatform>
        
        <!-- Platforms where we are -->
        <em:targetPlatform>WINNT_x86-msvc</em:targetPlatform>
        <em:targetPlatform>Linux</em:targetPlatform>
    </Description>      
</RDF>
